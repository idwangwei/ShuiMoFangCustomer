
.no-order{
    width: 100%;
    position: absolute;
    bottom: 0;
    top: 88rpx;
    left: 0;
    right: 0;
    text-align: center;
    padding-top: 203rpx;
    background-color: #F2f2f2;
}
.no-order-img{
    width: 81rpx;
    height: 96rpx;
    margin-bottom: 31rpx;
}
.no-order .text{
    font-size:28rpx;
    color:#999999;
    text-align: center
}
.order-list-item{
    margin:15rpx 0;
    border-radius: 30rpx;
    position:initial;

}
.order-item-bottom{
    padding-left:30rpx;
    padding-right: 30rpx;
    display:flex;
    width:100%;
    justify-content:space-between;
    font-size:28rpx;

}
.item-color-name{
    color: #404040;
    display:flex;
    align-items:center;
    padding-right:10rpx;
    text-align-last:left;

}
.display-between{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.arrow-right{
    border-right: 1rpx solid #626262;
    border-top: 1rpx solid #626262;
    width:20rpx;
    height:20rpx;
    transform:rotate(45deg);
}
page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-navbar{
    background-color: #ffffff;
}
.weui-tab__panel{
    background-color:#F2f2f2
}
.weui-tab__panel::-webkit-scrollbar{
    display: none;
}